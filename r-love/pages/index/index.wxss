.index{width:100vw;height:100vh}.index .love-tabbar{width:70vw;height:55px;background:#fff;-webkit-backdrop-filter:blur(3px);backdrop-filter:blur(3px);border-radius:50px;position:fixed;bottom:25px;left:15vw;display:flex;justify-content:space-evenly;box-shadow:0 0 20px rgba(0,0,0,.08)}.index .love-tabbar .love-tabbar-item .love-tabbar-item-icon{width:30px;height:30px;border-radius:50px;margin:5px auto 0}.index .love-tabbar .love-tabbar-item .title{font-size:12px;text-align:center;line-height:5px}.index .love-tabbar .love-tabbar-item .select{animation:shake .5s;animation-iteration-count:1;color:#f04b3f;font-weight:900}.index .love-tabbar .love-tabbar-item .noselect{color:#bfbfbf}@keyframes shake{0%{transform:translate(1px,1px) rotate(0)}10%{transform:translate(-1px,-2px) rotate(-1deg)}20%{transform:translate(-3px) rotate(1deg)}30%{transform:translate(3px,2px) rotate(0)}40%{transform:translate(1px,-1px) rotate(1deg)}50%{transform:translate(-1px,2px) rotate(-1deg)}60%{transform:translate(-3px,1px) rotate(0)}70%{transform:translate(3px,1px) rotate(-1deg)}80%{transform:translate(-1px,-1px) rotate(1deg)}90%{transform:translate(1px,2px) rotate(0)}to{transform:translate(1px,-2px) rotate(-1deg)}}
